{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "es2020",
    "target": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    
    "baseUrl": "./",
    "paths": {
      "@src/*": [ "src/*" ],
      "@util/*": [ "src/util/*" ],
    }
  }
}